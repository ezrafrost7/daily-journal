{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"]
    },
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noUnusedLocals": false,
    "skipLibCheck": true
  },
  "include": [
    "src/**/*",
    "index.js",
    "App.tsx"
  ]
}
